{
    "name": "dranzd/storebunk-accounting",
    "description": "Storebunk Accounting library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "dranzd/common-event-sourcing": "dev-main",
        "dranzd/common-utils": "dev-main",
        "dranzd/common-domain-assert": "dev-main",
        "dranzd/common-valueobject": "dev-main",
        "dranzd/common-cqrs": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.8"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:dranzd/common-event-sourcing.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dranzd/common-utils.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dranzd/common-domain-assert.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dranzd/common-valueobject.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dranzd/common-cqrs.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dranzd\\StorebunkAccounting\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
